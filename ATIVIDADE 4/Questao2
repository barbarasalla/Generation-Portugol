programa
{
	inclua biblioteca Util
	funcao inicio()
	{
		inteiro i, num[10], maiorNum=0, contMaior=0

		para (i=0; i<10; i++){
			num[i]= Util.sorteia(1,6)
			se (num[i]>maiorNum){
				maiorNum= num[i]
			}
		}
		
		para (i=0; i<10; i++){
			se (num[i]==maiorNum){
			contMaior++
			}
		}
		escreva("\n\nO maior número sorteado foi ", maiorNum)
		escreva("\nO maior número apareceu ", contMaior, " vezes.")
	}
}
